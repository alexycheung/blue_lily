<tr>
	<td class="nowrap">
		<%= fa_icon "circle", class: "#{status(property)} status" %>
	</td>
	<td>
		<%= property.address %>
	</td>
	<td>
		<%= property.city %>
	</td>
	<td>
		<%= property.state %>
	</td>
	<td><%= property.user.name if property.user %></td>
	<td><%= property.start_date.strftime("%D") if property.start_date %></td>
	<td><%= property.end_date.strftime("%D") if property.end_date %></td>
	<td class="nowrap">
		<%= link_to assign_property_path(property), class: "pure-button" do %>
			<%= fa_icon "bars" %>
		<% end %>
		<%= link_to edit_property_path(property), class: "pure-button" do %>
			<%= fa_icon "pencil" %>
		<% end %>
		<%= link_to property, method: :delete, data: { confirm: "Sure you want to delete this property? "}, class: "pure-button" do %>
			<%= fa_icon "trash" %>
		<% end %>
	</td>
</tr>