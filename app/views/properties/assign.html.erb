<div id="properties-assign">
	<h4>
		<%= @property.address %>, <%= @property.city %>, <%= @property.state %>
	</h4>
	<table class="pure-table">
		<thead>
			<tr>
				<th></th>
				<th>Name</th>
				<th>Size</th>
				<th>Color</th>
				<th>Category</th>
				<th>Condition</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% @items.each do |item| %>
				<%= render partial: "items/reserve", locals: { item: item } %>
			<% end %>
		</tbody>
	</table>
</div>